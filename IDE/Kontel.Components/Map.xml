<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Author />
  <Name>Map file</Name>
  <Desc>Syntax Scheme for map files</Desc>
  <Copyright />
  <FileExtension>.map</FileExtension>
  <FileType>map</FileType>
  <Version>1.1</Version>
  <Styles>
    <Style>
      <Name>numbers</Name>
      <ForeColor>DarkSlateGray</ForeColor>
      <Index>1</Index>
    </Style>
    <Style>
      <Name>idents</Name>
      <ForeColor>Teal</ForeColor>
      <Index>0</Index>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Blue</ForeColor>
      <Index>2</Index>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Green</ForeColor>
      <Index>3</Index>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>xml_comments</Name>
      <ForeColor>Silver</ForeColor>
      <Index>4</Index>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>symbol</Name>
      <ForeColor>Gray</ForeColor>
      <Index>5</Index>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>WindowText</ForeColor>
      <Index>6</Index>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>SeaGreen</ForeColor>
      <Index>7</Index>
    </Style>
    <Style>
      <Name>directives</Name>
      <ForeColor>Blue</ForeColor>
      <Index>8</Index>
    </Style>
    <Style>
      <Name>misc</Name>
      <ForeColor>MediumSeaGreen</ForeColor>
      <Index>9</Index>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\b[0-9A-Fa-f]+\b)|(0[xX][0-9A-Fa-f]+)|([0-9A-Fa-f]+[hH])</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name>Resword Set0</Name>
              <Index>0</Index>
              <ResWordStyle>9</ResWordStyle>
              <ResWords>
                <word>Hexadecimal</word>
                <word>Area</word>
                <word>Addr</word>
                <word>Size</word>
                <word>Decimal</word>
                <word>Bytes</word>
                <word>Attributes</word>
                <word>Value</word>
                <word>Global</word>
                <word>Module</word>
                <word>symbol</word>
                <word>kind</word>
                <word>val</word>
                <word>Local</word>
                <word>Symbol</word>
                <word>List</word>
              </ResWords>
            </ResWordSet>
            <ResWordSet>
              <Name>Resword Set1</Name>
              <Index>1</Index>
              <ResWordStyle>7</ResWordStyle>
              <ResWords>
                <word>Addr.</word>
                <word>Func.</word>
                <word>Var.</word>
                <word>EQU</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[A-Za-z_][0-9A-Za-z\\/\.\:]*</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>symbols</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\W</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>0</Index>
    </State>
  </States>
</LexScheme>